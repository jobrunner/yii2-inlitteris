{
  "name": "jobrunner/yii2-inlitteris",
  "description": "A simple reference manager module for Yii2",
  "keywords": ["yii2", "yii2-inlitteris", "reference management"],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Jo Brunner",
      "email": "jo@mett.io",
      "homepage": "http://mett.io"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "2.0.*",
    "yiisoft/yii2-bootstrap": "*"
  },
  "require-dev": {
      "phpunit/phpunit": "3.7.*"
  },
  "autoload": {
      "psr-4": {
        "jobrunner\\inlitteris\\": ""
      }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "suggest": {
    "academicpuma/citeproc-php": "Allows more advanced formatting options for this module"
  }
}
